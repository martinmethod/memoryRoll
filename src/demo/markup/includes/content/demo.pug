#example.data.cf
  p.info Choose a range and press the &quot;Generate&quot; button!
  .row.range.cf
    span.ttl Range
    .field.from
      label.lbl(for='example-range-from') From
      |
      input#example-range-from.textfield(type='number', min='0', max='999')
      br
    .field.to
      label.lbl(for='example-range-to') To
      |
      input#example-range-to.textfield(type='number', min='1', max='999')
  .row.buttons.cf
    input.btn.trigger(type='button', value='Generate')
    |
    input.btn.reset(type='button', value='Reset')
  .row.result.cf
    span.ttl
    strong.result
